using System;
using System.Collections.Generic;
using System.Text;

namespace Demo
{
    class Program
    {

        public static string getstr(string str){
            char[] delimit = new char[] { '3' };
	 	  string a = null;
		foreach (string substr in str.Split(delimit))
		{
           a +=substr;
		   	Console.WriteLine(substr);

		}

           return  a;
        }
        static void Main(string[] args)
        {
            string test ="3232323232";

            string c = null;
            c =getstr(test);
            	Console.WriteLine(c);

        }
    }
}
